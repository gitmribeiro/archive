{
  "name": "archive",
  "version": "1.0.0",
  "description": "WTT - Backup Server",
  "author": "WTT",
  "license": "ISC",
  "main": "build/index.js",
  "bin": "build/index.js",
  "keywords": [],
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "data/resources/**/*"
  },
  "scripts": {
    "start": "node build",
    "tsc": "tsc --watch",
    "eslint": "cls && eslint --ext .ts ./src || echo Completed!",
    "doc": "cls && typedoc ./src --out ./resources/doc",
    "build": "cls && pkg package.json -t node8-win --output ./dist/wttarchive.exe",
    "test": "tsc && mocha build/**/*.spec.js"
  },
  "dependencies": {
    "aws-sdk": "^2.382.0",
    "eslint-plugin-typescript": "^0.14.0",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "readdir-enhanced": "^2.2.4",
    "readline": "^1.3.0",
    "typescript": "^3.2.2",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.5.1"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.18",
    "@types/node-cron": "^2.0.0",
    "@types/readdir-enhanced": "^2.2.0",
    "@types/winston": "^2.4.4",
    "eslint": "^5.11.1",
    "typedoc": "^0.14.0",
    "typescript-eslint-parser": "^21.0.2"
  }
}
